<link href="css/bstreeview.css" rel="stylesheet">

<div id="all" class="card">
	<div class="row">
		<div class="col-sm-12">
			<h5 class="card-header">Secuence information</h5>
			<div class="card-body">
			{{#each peptides}}<h5 class="card-title">Sequence:</h5><p class="card-text">{{'sequence'}}</p>{{/each}}
			{{#each peptides}}<h5 class="card-title">Sequence ID:</h5><p class="card-text">{{'id_sequence'}}</p>{{/each}}
			{{#each peptides}}<h5 class="card-title">Length:</h5><p class="card-text">{{'length'}}</p>{{/each}}
			{{#each peptides}}<h5 class="card-title">Molecular weight:</h5><p class="card-text">{{'molecular_weigth'}}</p>{{/each}}
			{{#each peptides}}<h5 class="card-title">Isoelectric:</h5><p class="card-text">{{'isoelectric_point'}}</p>{{/each}}
			{{#each peptides}}{{#if peptide_name}}<h5 class="card-title">Peptide name:</h5><p class="card-text">{{'peptide_name'}}</p>{{/if}}{{/each}}
			{{#each peptides}}<h5 class="card-title">Charge:</h5><p class="card-text">{{'charge'}}</p>{{/each}}
			{{#each peptides}}<h5 class="card-title">Charge density:</h5><p class="card-text">{{'charge_density'}}</p>{{/each}}
			{{#each peptides}}<h5 class="card-title">Is modified:</h5><p class="card-text">{{'is_modify'}}</p>{{/each}}
			</div>
      	</div>
		<div class="card-group col-sm-12">
			<div class="card">
				<h5 class="card-header">Properties information</h5>
				<div class="card-body">
