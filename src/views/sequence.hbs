				{{#each peptides}}{{#if taxonomy}}<h5 class="card-title">taxonomy:</h5><p class="card-text">{{'taxonomy'}}</p>{{/if}}{{/each}}
				{{#each peptides}}{{#if formula}}<h5 class="card-title">formula:</h5><p class="card-text">{{'formula'}}</p>{{/if}}{{/each}}
				</div>
			</div>
		</div>

		  <div class="col-sm-12" id="otherInfo">
			<h5 class="card-header">Other information</h5>
			<div class="card-body">
			{{#each peptides}}{{#if Virus_antiviral}}<h5 id="Virus_antiviral" class="card-title">Virus_antiviral:</h5><p class="card-text">{{'Virus_antiviral'}}</p>{{/if}}{{/each}}
