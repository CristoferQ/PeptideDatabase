				{{#each peptides}}<h5 class="card-title">Hydrophobic ratio:</h5><p class="card-text">{{'hydrophobic_ratio'}}</p>{{/each}}
				{{#each peptides}}<h5 class="card-title">Hydrophobicity profile:</h5><p class="card-text">{{'hydrophobicity_profile'}}</p>{{/each}}
				{{#each peptides}}<h5 class="card-title">Hydrophobic profile:</h5><p class="card-text">{{'hydrophobic_profile'}}</p>{{/each}}
				{{#each peptides}}<h5 class="card-title">Momment:</h5><p class="card-text">{{'momment'}}</p>{{/each}}
				</div>	
			</div>
			<div class="card">
				<h5 class="card-header">Taxonomy</h5>
				<div class="card-body">
				{{#each peptides}}{{#if organism_value}}<h5 class="card-title">Organism:</h5><p class="card-text">{{'organism_value'}}</p>{{/if}}{{/each}}
